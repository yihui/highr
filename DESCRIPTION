Package: highr
Type: Package
Title: Syntax Highlighting for R Source Code
Version: 0.10.1
Authors@R: c(
    person("Yihui", "Xie", role = c("aut", "cre"), email = "xie@yihui.name", comment = c(ORCID = "0000-0003-0645-5666")),
    person("Yixuan", "Qiu", role = "aut"),
